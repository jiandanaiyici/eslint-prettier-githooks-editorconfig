    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.0",
    "lint-staged": "^11.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": ["eslint . --fix", "prettier -c --write"]
